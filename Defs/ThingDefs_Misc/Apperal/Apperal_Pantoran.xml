<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingDef ParentName="ApparelMakeableBase">
      <defName>Apparel_PantoranPrincessOutfit</defName>
        <label>Princess Outfit</label>
        <description>The outfit Pantoran Princess wore.</description>
        <techLevel>Spacer</techLevel>
        <recipeMaker>
            <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
            <recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
        </recipeMaker>
        <graphicData>
            <texPath>Things/Item/Equipment/Apparel/PrincessOutfit/TGPrincess</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <costStuffCount>80</costStuffCount>
        <stuffCategories>
            <li>Fabric</li>
            <li>Leathery</li>
        </stuffCategories>
        <costList>
            <Steel>10</Steel>
        </costList>
        <thingCategories>
            <li>Apparel</li>
        </thingCategories>
        <statBases>
            <Mass>1</Mass>
			<MaxHitPoints>350</MaxHitPoints>
			<MarketValue>100</MarketValue>
			<ArmorRating_Blunt>0.3</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.3</ArmorRating_Sharp>
			<ArmorRating_Heat>0.3</ArmorRating_Heat>
			<StuffEffectMultiplierInsulation_Cold>0.15</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.05</StuffEffectMultiplierInsulation_Heat>
        </statBases>
        <apparel>
            <bodyPartGroups>            
				<li>Torso</li>
				<li>Arms</li>
				<li>Legs</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Equipment/Apparel/PrincessOutfit/TGPrincess</wornGraphicPath>
            <layers>
                <li>OnSkin</li>
            </layers>
            <tags>
                <li>Outlander</li>
                <li>Spacer</li>
                <li>Princess_Pantoran</li>
            </tags>
            <defaultOutfitTags>
                <li>Worker</li>
                <li>Soldier</li>
            </defaultOutfitTags>
        </apparel>
    </ThingDef>
</Defs>